{
  "name": "npm-fao-component",
  "version": "0.0.1",
  "description": "npm-component",
  "keywords": [
    "npm",
    "npm-boilerplate",
    "react",
    "react-component"
  ],
  "repository": "https://github.com/fedorovsky/npm-boilerplate",
  "license": "MIT",
  "author": "Anton Fedorovskyi <anton.fedorovsky@gmail.com>",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "postcss": "postcss ./src/theme.css -o ./dist/theme.css",
    "dev": "webpack-dev-server",
    "js": "babel --no-babelrc ./src -d ./dist/ --ignore '**/*.test.js' --presets=babili,env,stage-0,react",
    "prepublish": "yarn run clean && yarn run postcss && yarn run js",
    "stylelint:--fix": "stylelint ./**/*.css !./dist/**/*.css --fix",
    "test": "jest --env=jsdom --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babili": "^0.1.4",
    "classnames": "^2.2.5",
    "clean-css-cli": "^4.1.11",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "path": "^0.12.7",
    "postcss-cli": "^5.0.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "recompose": "^0.27.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
